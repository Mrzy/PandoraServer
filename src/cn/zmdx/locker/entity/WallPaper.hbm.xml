<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="cn.zmdx.locker.entity.WallPaper" table="wallpaper">
		<id name="id" column="id" type="int">
			<generator class="native" />
		</id>
		<property name="name" column="name" />
		<property name="desc" column="desc" />
		<property name="author" column="author" />
		<property name="thumbURL" column="thumbURL" />
		<property name="imageURL" column="imageURL" />
		<property name="imageNAME" column="imageNAME" />
		<property name="imageEXT" column="imageEXT" />
		<property name="publishDATE" column="publishDATE" />
	</class>

</hibernate-mapping>
  