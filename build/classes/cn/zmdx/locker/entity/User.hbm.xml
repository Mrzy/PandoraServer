<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="cn.zmdx.locker.entity.User" table="testdata">
	  <id name="worker_no" column="id" type="int" >
			<generator class="native" />
		</id>
		<property name="worker_phone" column="tou" />	
		<property name="worker_name" column="imgUrl"/>
		<property name="identitycard_no" column="no" />	
		<property name="area_no" column="no1" />	
		<property name="password" column="neirong" />		
	</class>
	
</hibernate-mapping>
  